<Window x:Class="Freengy.UI.Windows.RegistrationWindow" 
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        
        xmlns:local="clr-namespace:Freengy.UI.Views"
        
        mc:Ignorable="d" 
        KeyUp="Window_KeyUp"
        ShowInTaskbar="False"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterOwner"
        Style="{StaticResource CommonWindowStyle}"
        Background="{StaticResource ThirdMainBrush}"
        Icon="pack://application:,,,/Freengy.CommonResources;component/Images/AccountIcon.png"
        Width="400" Height="340" MinWidth="400" MinHeight="290" MaxWidth="500" MaxHeight="360">
    
    <local:RegistrationView/>
</Window>