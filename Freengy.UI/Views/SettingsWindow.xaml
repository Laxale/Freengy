<Window x:Class="Freengy.UI.Views.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        
        xmlns:commonres="clr-namespace:Freengy.CommonResources;assembly=Freengy.CommonResources"
        
        xmlns:local="clr-namespace:Freengy.UI.Views"
        
        mc:Ignorable="d"
        Title="{x:Static commonres:StringResources.SettingsText}" 
        Icon="pack://application:,,,/Freengy.CommonResources;component/Images/SettingsIcon.png"
        Height="300" Width="300" 
        WindowStartupLocation="CenterOwner" ShowInTaskbar="False">

    <local:SettingsView/>
</Window>