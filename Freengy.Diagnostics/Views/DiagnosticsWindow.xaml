        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             
        xmlns:local="clr-namespace:Freengy.Diagnostics.Views"
        
        mc:Ignorable="d" 
        Style="{StaticResource CommonWindowStyle}"
