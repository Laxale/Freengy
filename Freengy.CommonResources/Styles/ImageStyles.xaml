<ResourceDictionary x:Class="Freengy.CommonResources.Styles.ImageStyles"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Variables.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    
    <Style TargetType="Image" x:Key="CommonImageStyle">
        <Setter Property="Margin" Value="{StaticResource All4Margin}"/>
    </Style>

    <Style TargetType="Image" BasedOn="{StaticResource CommonImageStyle}">

    </Style>
</ResourceDictionary>